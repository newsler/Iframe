javascript:(()=>{alert("Made by TheNewsler\nThis script opens an iframe which bypasses Guardian's whitelist. The whitelist is the message that says \"Hey there! it looks like you reached a website your teacher doesn't want you to browse now.\"");var o=prompt("What would you like to open?\nOption 1: Thenewladder.com\nOption 2: Bing.com\nOption 3: Custom");if(o==="1"||o==="2"||o==="3"){var i=document.createElement("iframe");i.setAttribute("id","rusic-modal");i.setAttribute("style","position:fixed;z-index:999999;width:100vw;height:100vh;border:none;");if(o==="1")i.setAttribute("src","https://newsler.wixsite.com/thenewsladder/");else if(o==="2")i.setAttribute("src","https://bing.com");else if(o==="3"){var u=prompt("Enter a website URL:");if(u!==null){u=u.trim();if(!u.startsWith("https://"))u="https://"+u;var r=/^https?:\/\/[\w\-]+(\.[\w\-]+)+[/#?]?.*$/;if(r.test(u))i.setAttribute("src",u);else{alert("Invalid URL. Please enter a valid website URL.");return;}}else{return;}}document.body.appendChild(i);i.requestFullscreen();}else alert("Invalid option. Please choose a valid option (1, 2, or 3).");})();
